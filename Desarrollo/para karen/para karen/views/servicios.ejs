              <form class="text-left px-5 pr-4" action="/servicio/add" method="POST">
                <div class="form-group"> <input type="text" name="ser_con" placeholder="Nombre de Servicio" class="form-control form-control-sm w-75" id="nombreServicio" value="" style="	text-shadow: 0px 0px 0px gray;"> </div>
                <div class="form-group"> <input type="text" name="ser_unidad" placeholder="Unidad de medida" class="form-control form-control-sm w-75" id="unidadMedida" style="	text-shadow: 0px 0px 0px grey;"> </div>
                <button type="submit" class="btn btn-success mx-4 px-4 pr-3">Aceptar</button><button type="submit" class="btn btn-dark mx-4 px-3 pr-4 text-center" style="">Cancelar</button>
              </form>
            </div>
            <!-- formulario  -->
            <div class="col-md-6" style="	background-image: url(&quot;../../assets/img/bg1.jpg&quot;);	background-position: top left;	background-size: 50%;	background-repeat: repeat;">
              <div class="row">
                <div class="col-md-9" style="">
                  <!-- formulario  -->
                </div>
              </div>
            </div>
          </div>
          <!-- copiar hasta -->
        </div>

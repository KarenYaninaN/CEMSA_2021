  <div class="py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <table class="table table-bordered ">
            <thead class="thead-dark">
              <tr>
                <th>#</th>
                <th>Nombre</th>
                <th>Unidad</th>
                <th>Acciones</th>
                                    </tr>
            </thead>
            <tbody>
              <% if (data) { %>
                <% for(var i = 0; i < data.length; i++) { %>
                  <tr>
                    <td><%= (i + 1) %></td>
                    <td><%= data[i].ser_con %></td>
                    <td><%= data[i].ser_unidad %></td>
                    <td>
                      <a href="/servicio/update/<%= data[i].ser_cod %>" class="btnEditar fa fa-pencil-square-o fa-lg mx-2"></a>
                      <a href="/servicio/delete/<%= data[i].ser_cod %>" class="fa fa-times fa-lg bg-success mx-2"></a>
                          
                      
                      
                     </td>

                    


                  </tr>
                <% } %>
              <% } %>
            </tbody>
          </table>
          </div>
        </div>
      </div>
    </div>

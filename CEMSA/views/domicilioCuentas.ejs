                   <div class="form-group"> <label for="form17">Nro Calle</label> <input type="text" name="dom_nro" placeholder="Nro Calle" class="form-control form-control-sm" id="dom_nro"> </div>
                   <div class="form-group"> <label for="form16">Manzana</label> <input type="text" name="dom_manzana" placeholder="Manzana" class="form-control form-control-sm" id="dom_manzana" value=""> </div>
                               

                <table class="table table-bordered ">
                  <thead class="thead-dark">
                    <tr>
                      <th>#</th>
                      <th>Calle</th>
                      <th>Numero Calle</th>
                      <th>Manzana</th>
                      <th>CP</th>
                      <th>Observación</th>
                      <th>Fecha Alta</th>
                      <th>Coordenada X</th>
                      <th>Coordenada Y</th>
                      <th>ID localidad</th>
                      <th>ID Usuario</th>
                      <th>Acciones</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if (dataDomicilioCuenta) { %>
                      <% for(var i = 0; i < dataDomicilioCuenta.length; i++) { %>
                        <tr>
                          <td><%= (i + 1) %></td>
                          <td><%= dataDomicilioCuenta[i].dom_calle %></td>
                          <td><%= dataDomicilioCuenta[i].dom_nro %></td>
                          <td><%= dataDomicilioCuenta[i].dom_manzana %></td>
                          <td><%= dataDomicilioCuenta[i].dom_cp %></td>
                          <td><%= dataDomicilioCuenta[i].dom_observacion %></td>
                          <td><%= dataDomicilioCuenta[i].dom_fechaAlta %></td>
                          <td><%= dataDomicilioCuenta[i].dom_coorX %></td>
                          <td><%= dataDomicilioCuenta[i].dom_coorY %></td>
                          <td><%= dataDomicilioCuenta[i].dom_idLocalidad %></td>
                          <td><%= dataDomicilioCuenta[i].dom_usuId %></td>
                          <td>
                            <a href="/domicilioCuenta/update/<%= dataDomicilioCuenta[i].dom_id %>" class="fa fa-pencil-square-o fa-lg mx-2"></a>
                            <a href="/domicilioCuenta/delete/<%= dataDomicilioCuenta[i].dom_id %>" class="fa fa-times fa-lg bg-success mx-2"></a>
                          </td>

                          


                        </tr>
                      <% } %>
                    <% } %>
